{
  "name": "toy-language",
  "displayName": "Toy Language",
  "description": "Syntax highlighting and language support for the Toy programming language",
  "version": "0.1.0",
  "publisher": "habond",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "toy",
    "compiler",
    "syntax highlighting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/habond/toy-compiler"
  },
  "license": "MIT",
  "contributes": {
    "languages": [
      {
        "id": "toy",
        "aliases": [
          "Toy",
          "toy"
        ],
        "extensions": [
          ".toy"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "toy",
        "scopeName": "source.toy",
        "path": "./syntaxes/toy.tmLanguage.json"
      }
    ]
  }
}
