// Test comparison operators
x = 5;
y = 10;

// Equality
result = x == y;
println result;

result = x != y;
println result;

// Less than
result = x < y;
println result;

result = y < x;
println result;

// Less than or equal
result = x <= y;
println result;

result = x <= 5;
println result;

// Greater than
result = x > y;
println result;

result = y > x;
println result;

// Greater than or equal
result = y >= x;
println result;

result = x >= 5;
println result;

// Complex expressions
result = x + 1 < y - 2;
println result;

result = x * 2 == y;
println result;
